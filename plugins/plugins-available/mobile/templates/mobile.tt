<!DOCTYPE html>
<html>
  <head>
    <title>Mobile Thruk</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel='stylesheet' type='text/css' href='[% url_prefix %]thruk/plugins/mobile/mobile.css' />
    <link rel="stylesheet" href="[% url_prefix %]thruk/plugins/mobile/jqm/jquery.mobile-1.0.min.css" />
    <script src="[% url_prefix %]thruk/javascript/jquery-1.6.2.min.js"></script>
    <script type="application/x-javascript">
      var url_prefix = '[% url_prefix %]';
    </script>
    <script src="[% url_prefix %]thruk/plugins/mobile/mobile.js" type="application/x-javascript" charset="utf-8"></script>
    <script src="[% url_prefix %]thruk/plugins/mobile/jqm/jquery.mobile-1.0.min.js"></script>
  </head>
  <body>

<!-- Home -->
    <div id="home" data-role="page">
      <div data-role="header" data-position="inline">
        <a href="#refresh" id="refresh" data-icon="refresh" class="ui-btn-left" data-iconpos="notext">Refresh</a>
        <h1>Mobile Thruk</h1>
        <a href="#options" data-icon="gear" class="ui-btn-right">Options</a>
      </div>
      <div data-role="content">
        <h4>Status</h4>
          <ul data-role="listview" data-inset="true">
            <li>
              <a href="#hosts">Hosts
              <span class="ui-li-count ui-btn-up-c ui-btn-corner-all hosts_pending_panel hostPENDING hosts_pending"></span>
              <span class="ui-li-count ui-btn-up-c ui-btn-corner-all hosts_unreachable_panel hostUNREACHABLE hosts_unreachable"></span>
              <span class="ui-li-count ui-btn-up-c ui-btn-corner-all hosts_down_panel hostDOWN hosts_down"></span>
              <span class="ui-li-count ui-btn-up-c ui-btn-corner-all hosts_up_panel hostUP hosts_up"></span>
              </a>
            </li>

            <li>
              <a href="#services">Services
                <span class="ui-li-count ui-btn-up-c ui-btn-corner-all services_pending_panel servicePENDING services_pending"></span>
                <span class="ui-li-count ui-btn-up-c ui-btn-corner-all services_critical_panel serviceCRITICAL services_critical"></span>
                <span class="ui-li-count ui-btn-up-c ui-btn-corner-all services_unknown_panel serviceUNKNOWN services_unknown"></span>
                <span class="ui-li-count ui-btn-up-c ui-btn-corner-all services_warning_panel serviceWARNING services_warning"></span>
                <span class="ui-li-count ui-btn-up-c ui-btn-corner-all services_ok_panel serviceOK services_ok"></span>
              </a>
            </li>
          </ul>

          <!--
          TODO:
          <h4>Search</h4>
          <form method="post">
          <div style="position:relative">
          <input type="text" name="name" id="search" value=""  />
          <a href="#search" class="" style="position: absolute; top: 5px; right:8px;"><img src="[% url_prefix %]thruk/plugins/mobile/img/06-search.png" alt="search"></a>
          </div>
          -->

          </form>
          <h4>History</h4>
          <ul data-role="listview" data-inset="true">
            <li><a href="#last_alerts">Last Alerts</a></li>
            <li><a href="#last_notification">Last Notifications</a></li>
          </ul>

          <h4>Links</h4>
          <ul data-role="listview" data-inset="true">
            <li><a href="[% url_prefix %]thruk/index.html" rel="external">Access as Full Client</a></li>
          </ul>
        </div>
        <div data-role="footer" align="center">
          Add this page to your home screen to get full screen mode.
      </div>
    </div>


<!-- Options -->
    <div id="options" data-role="page">
      <div data-role="header">
        <a href="#" data-rel="back">Back</a>
        <h1>Options</h1>
      </div>
      <div data-role="content">
      </div>
    </div>

<!-- Hosts by Status-->
    <div id="hosts" data-role="page">
      <div data-role="header">
        <a href="#" data-rel="back">Back</a>
        <h1>Hosts</h1>
      </div>
      <div data-role="content">
        <ul data-role="listview" data-inset="true" id="hosts_by_status_list">
          <li class="hidden hosts_up_panel"><a href="#hosts_list" class="hosts_list">Up<span class="ui-li-count ui-btn-up-c ui-btn-corner-all hostUP hosts_up"></span></a></li>
          <li class="hidden hosts_down_panel"><a href="#hosts_list" class="hosts_list">Down<span class="ui-li-count ui-btn-up-c ui-btn-corner-all hostDOWN hosts_down"></span></a></li>
          <li class="hidden hosts_unreachable_panel"><a href="#hosts_list" class="hosts_list">Unreachable<span class="ui-li-count ui-btn-up-c ui-btn-corner-all hostUNREACHABLE hosts_unreachable"></span></a></li>
          <li class="hidden hosts_pending_panel"><a href="#hosts_list" class="hosts_list">Pending<span class="ui-li-count ui-btn-up-c ui-btn-corner-all hostPENDING hosts_pending"></span></a></li>
        </ul>
      </div>
    </div>

<!-- Services by Status-->
    <div id="services" data-role="page">
      <div data-role="header">
        <a href="#" data-rel="back">Back</a>
        <h1>Services</h1>
      </div>
      <div data-role="content">
        <ul data-role="listview" data-inset="true" id="services_by_status_list">
          <li class="hidden services_ok_panel"><a href="#services_list" onclick="filter={servicestatustypes:2};">Ok<span class="ui-li-count ui-btn-up-c ui-btn-corner-all serviceOK services_ok"></span></a></li>
          <li class="hidden services_warning_panel"><a href="#services_list" onclick="filter={servicestatustypes:4};">Ok<span class="ui-li-count ui-btn-up-c ui-btn-corner-all serviceWARNING services_warning"></span></a></li>
          <li class="hidden services_critical_panel"><a href="#services_list" onclick="filter={servicestatustypes:16};">Ok<span class="ui-li-count ui-btn-up-c ui-btn-corner-all serviceCRITICAL services_critical"></span></a></li>
          <li class="hidden services_unknown_panel"><a href="#services_list" onclick="filter={servicestatustypes:8};">Ok<span class="ui-li-count ui-btn-up-c ui-btn-corner-all serviceUNKNOWN services_unknown"></span></a></li>
          <li class="hidden services_pending_panel"><a href="#services_list" onclick="filter={servicestatustypes:1};">Ok<span class="ui-li-count ui-btn-up-c ui-btn-corner-all servicePENDING services_pending"></span></a></li>
        </ul>
      </div>
    </div>

<!-- Hosts List -->
    <div id="hosts_list" data-role="page">
      <div data-role="header">
        <a href="#" data-rel="back">Back</a>
        <h1>Hosts</h1>
      </div>
      <div data-role="content">
        <ul data-role="listview" id="hosts_list_data" data-filter="true">
          <li><img src="[% url_prefix %]thruk/plugins/mobile/img/loading.gif" alt="loading"> loading</li>
        </ul>
      </div>
    </div>

<!-- Services List -->
    <div id="services_list" data-role="page">
      <div data-role="header">
        <a href="#" data-rel="back">Back</a>
        <h1>Services</h1>
      </div>
      <div data-role="content">
        <ul data-role="listview" id="services_list_data" data-filter="true">
          <li><img src="[% url_prefix %]thruk/plugins/mobile/img/loading.gif" alt="loading"> loading</li>
        </ul>
      </div>
    </div>

<!-- Last Alerts List -->
    <div id="last_alerts" data-role="page">
      <div data-role="header">
        <a href="#" data-rel="back">Back</a>
        <h1>Alerts</h1>
      </div>
      <div data-role="content">
        <ul data-role="listview" id="alerts_list" data-filter="true">
          <li><img src="[% url_prefix %]thruk/plugins/mobile/img/loading.gif" alt="loading"> loading</li>
        </ul>
      </div>
    </div>

<!-- Last Notification List -->
    <div id="last_notification" data-role="page">
      <div data-role="header">
        <a href="#" data-rel="back">Back</a>
        <h1>Notifications</h1>
      </div>
      <div data-role="content">
        <ul data-role="listview" id="notification_list" data-filter="true">
          <li><img src="[% url_prefix %]thruk/plugins/mobile/img/loading.gif" alt="loading"> loading</li>
        </ul>
      </div>
    </div>


  </body>
</html>
